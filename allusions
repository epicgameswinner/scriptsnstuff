local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/VisualRoblox/Roblox/main/UI-Libraries/Visual%20UI%20Library/Source.lua"))()

local Window = Library:CreateWindow('Kyero + Edgar Hub', 'Paid Allusions Script', 'Kyero + Edgar', 'rbxassetid://10618928818', false, 'VisualUIConfigs', 'Default')

local Tab1 = Window:CreateTab('Stat Changer', true, 'rbxassetid://3926305904', Vector2.new(524, 44), Vector2.new(36, 36))
local Tab2 = Window:CreateTab('Retake Mode', true, 'rbxassetid://3926305904', Vector2.new(524, 44), Vector2.new(36, 36))
local Tab3 = Window:CreateTab('How to use', true, 'rbxassetid://3926305904', Vector2.new(524, 44), Vector2.new(36, 36))

local Section1 = Tab1:CreateSection('Stat Changer')
local Section2 = Tab2:CreateSection('Retake Mode')
local Section3 = Tab3:CreateSection('How to use')

local Toggle = Section1:CreateToggle('Enable Stat Changer', false, Color3.fromRGB(0, 125, 255), 0.25, function(Value1)
    _G.StatChanger = Value1

    while _G.StatChanger do
        wait(2)
        


        if getgenv().a == true then
        for i,v in pairs(game.Players.LocalPlayer.Character:GetChildren()) do
            if v.Name == "Red" or v.Name == "Blue" or v.Name == "Yellow" then
                v:Destroy()
                print(a)
            end
        end
        end
        if getgenv().d == true then
        for i,v in pairs(game:GetService("Lighting"):GetChildren()) do
            if v.Name == "Atmosphere" then
                print(d)
                v:Destroy()
            end
        end
            if v.Name == "Dozerify" or v.Name == "RussianTripp" or v.Name == "HeartBeat" then
                if not v:IsA("Tool") then
                    v:Destroy()
                end
            end
        end
    end
end)


local Toggle = Section1:CreateToggle('Infinite Russian Vitamines + Dozerify', true, Color3.fromRGB(0, 125, 255), 0.25, function(k)
        getgenv().d = k
   end)

local Toggle = Section1:CreateToggle('Candy Bag', true, Color3.fromRGB(0, 125, 255), 0.25, function(l)
        getgenv().a = l
   end)

